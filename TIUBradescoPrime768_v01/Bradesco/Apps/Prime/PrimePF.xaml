		</EventTrigger>
	</UserControl.Triggers>

	<Canvas Name="canvasTarifa"  ClipToBounds="True"  >
		<Image Width="768" Height="1030" Source="/Bradesco;component/Imagens/prime/indice_prime_pf.png" Stretch="UniformToFill" />

		<ContentControl Tag="0" Content="" IsManipulationEnabled="True" Canvas.Left="20" Canvas.Top="10" Height="38" Width="727" TouchDown="ContentControl_TouchDown" MouseDown="ContentControl_TouchDown"/>
        <ContentControl Tag="475"   Content="" IsManipulationEnabled="True" Canvas.Left="20" Canvas.Top="58" Height="38" Width="727" TouchDown="ContentControl_TouchDown" MouseDown="ContentControl_TouchDown"/>
        <ContentControl Tag="3336"  Content="" IsManipulationEnabled="True" Canvas.Left="20" Canvas.Top="106" Height="38" Width="727" TouchDown="ContentControl_TouchDown"  MouseDown="ContentControl_TouchDown"/>
        <ContentControl Tag="4729"  Content="" IsManipulationEnabled="True" Canvas.Left="20" Canvas.Top="154" Height="38" Width="727" TouchDown="ContentControl_TouchDown"  MouseDown="ContentControl_TouchDown"/>
        <ContentControl Tag="12296" Content="" IsManipulationEnabled="True" Canvas.Left="20" Canvas.Top="202" Height="38" Width="727" TouchDown="ContentControl_TouchDown"  MouseDown="ContentControl_TouchDown"/>
        <ContentControl Tag="13363" Content="" IsManipulationEnabled="True" Canvas.Left="20" Canvas.Top="250" Height="38" Width="727" TouchDown="ContentControl_TouchDown"  MouseDown="ContentControl_TouchDown"/>
        <ContentControl Tag="14072" Content="" IsManipulationEnabled="True" Canvas.Left="20" Canvas.Top="298" Height="38" Width="727" TouchDown="ContentControl_TouchDown"  MouseDown="ContentControl_TouchDown"/>

		<TextBlock x:Name="txtVigencia" Canvas.Left="203" TextWrapping="Wrap" Canvas.Top="902" Height="18" Width="240" Background="#00A0A2A5" Foreground="White" FontFamily="NewJuneBold" FontSize="14"/>
		<TextBlock x:Name="txtTarifas"  Canvas.Left="44" TextWrapping="Wrap" Canvas.Top="1000" Height="13" Width="676" Background="#00A6A8AB" Foreground="White" FontFamily="NewJuneMedium" FontSize="10"/>        

