<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        x:Class="Bradesco.MainWindow"
        ResizeMode="NoResize" WindowStartupLocation="CenterScreen" WindowState="Maximized" WindowStyle="None" >
    <ScrollViewer VerticalScrollBarVisibility="Hidden" HorizontalScrollBarVisibility="Hidden">
        <Grid>
            <MediaElement Name="screenSaver" Stretch="UniformToFill" Height="1920" Width="1080" LoadedBehavior="Manual" IsManipulationEnabled="True" Visibility="Hidden"/>
            <Canvas x:Name="canvas"></Canvas>
        </Grid>
    </ScrollViewer>
    <!--<local:Main HorizontalAlignment="Left" Margin="0,0,-10,-8" VerticalAlignment="Top" FontFamily="NewJuneBook"/>-->

</Window>
